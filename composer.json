{
  "name": "dottedai/laravel-model-annotator",
  "description": "Laravel command to annotate Eloquent models with database schema, relationships, and casts.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Matheus Puppe",
      "email": "matheus.puppe@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "illuminate/support": "^9.0|^10.0|^11.0"
  },
  "autoload": {
    "psr-4": {
      "DottedAI\\ModelAnnotator\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DottedAI\\ModelAnnotator\\ModelAnnotatorServiceProvider"
      ]
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
